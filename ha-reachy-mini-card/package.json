  "description": "Home Assistant custom card for Reachy Mini 3D visualization",
  "main": "dist/ha-reachy-mini-card.js",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --environment BUILD:development",
    "watch": "rollup -c -w",
